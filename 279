 def fib_short(n):
    if n < 0: return []
    
    a, b = 0, 1
    result = []
    
    while a <= n:
        result.append(a)
        a, b = b, a + b
        
    return result

try:
    n_input = int(input("Введіть n: "))
    sequence = fib_short(n_input)
    
    print(f"Ряд Фібоначчі до {n_input}:")
    print(sequence)
except ValueError:
    print("Помилка: Необхідно ввести ціле число.")
